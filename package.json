{
  "name": "Schoolz",
  "version": "0.1.1",
  "description": "A webpack React starter",
  "repository": "https://github.com/sherlyc/Schoolz.git",
  "main": "server/index.js",
  "scripts": {
    "knex": "knex",
    "dev": "nodemon server/index & webpack --watch",
    "start": "node server/index",
    "debug": "node --inspect --debug-brk server/index.js",
    "test": "tape -r babel-register 'test/index.test.js' | tap-diff & ava 'test/db.test.js'",
    "test2": "ava 'test/db.test.js'",
    "watch": "nodemon -e js,jsx -x npm test"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Sherly",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.7.0",
    "jsx-loader": "^0.13.2",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.1.1",
    "sqlite3": "^3.1.8",
    "supertest": "^3.0.0",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.3",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "knex": "^0.13.0"
  }
}
