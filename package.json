{
  "name": "Schoolz",
  "version": "0.1.1",
  "description": "A webpack React starter",
  "repository": "https://github.com/sherlyc/Schoolz.git",
  "main": "server/index.js",
  "scripts": {
    "knex": "knex",
    "dev": "nodemon server/index & webpack --watch",
    "start": "node server/index",
    "debug": "node --inspect --debug-brk server/index.js",
    "test": "tape -r babel-register 'tests/api/index.test.js' | tap-diff & ava 'tests/db/db.test.js'",
    "watch": "nodemon -e js,jsx -x npm test"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "author": "Sherly",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.18.0",
    "enzyme": "^2.7.0",
    "jsx-loader": "^0.13.2",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-radio-group": "^3.0.1",
    "react-router-dom": "^4.1.1",
    "sqlite3": "^3.1.8",
    "supertest": "^3.0.0",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.3",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.3",
    "express": "^4.14.0",
    "jwt-simple": "^0.5.1",
    "knex": "^0.13.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^6.2.3",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-form": "^6.8.0",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.5.2"
  }
}
